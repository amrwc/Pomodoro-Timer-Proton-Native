{
  "name": "pomodoro-timer",
  "author": "https://github.com/amrwc",
  "version": "0.1.3",
  "licence": "MIT",
  "homepage": "https://github.com/amrwc/Pomodoro-Timer-Proton-Native",
  "description": "Simple Pomodoro Timer built in Proton Native",
  "keywords": [
    "proton-native",
    "react",
    "javascript",
    "pomodoro",
    "timer"
  ],
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/babel-node index.js",
    "build": "node_modules/.bin/babel index.js -d bin/",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "proton-native": "latest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-preset-env": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "latest",
    "electron-builder": "latest",
    "proton-native-reload": "^1.1.0"
  },
  "build": {
    "protonNodeVersion": "current",
    "appId": "com.pomodoro-timer",
    "productName": "Pomodoro Timer",
    "copyright": "Copyright Â© 2019 amrwc",
    "mac": {
      "identity": null,
      "category": "public.app-category.productivity"
    }
  }
}
